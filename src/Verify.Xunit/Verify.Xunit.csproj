<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net472</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.CodeDom" Version="4.7.0" />
    <PackageReference Include="xunit.assert" Version="2.4.1" />
    <PackageReference Include="XunitContext" Version="1.5.1" />
    <PackageReference Include="TextCopy" Version="2.0.0-beta.3" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="ProjectDefaults" Version="1.0.33" PrivateAssets="All" />
    <PackageReference Include="Nullable" Version="1.1.1" PrivateAssets="All" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Condition="$(Configuration) == 'Release'" Version="1.0.0" PrivateAssets="All" />
    <Content Include="buildMultiTargeting\Verify.Xunit.targets" PackagePath="buildMultiTargeting\Verify.Xunit.targets" />
    <Content Include="build\Verify.Xunit.targets" PackagePath="build\Verify.Xunit.targets" />
    <None Include="EmptyFiles\empty.*">
      <Pack>true</Pack>
      <PackagePath>EmptyFiles</PackagePath>
    </None>
  </ItemGroup>
</Project>