<?xml version="1.0" encoding="utf-8"?>
<Project Condition="$(DesignTimeBuild) != true">
  <PropertyGroup>

  </PropertyGroup>
  <ItemGroup>
    <None Update="**\*.received.*;**\*.verified.*">
      <DependentUpon>$(
        [System.String]::Copy('%(FileName)')
        .SubString(
        0,
        $([System.String]::Copy('%(FileName)').IndexOf('.'))
